{
  "name": "my-diary",
  "version": "1.0.0",
  "description": "My Diary is an online journal where users can pen down their thoughts and feelings.",
  "main": "app.js",
  "scripts": {
    "build": "babel server -d dist",
    "start": "NODE_ENV=production && npm install && npm run build && node dist/server.js",
    "clean": "rm -rf node_modules",
    "test:server": "NODE_ENV=Test mocha server/test/**/*.js --exit  --require @babel/register --reporter spec || true",
    "test": "npm run test:server && npm run coverage",
    "coverage":"nyc report --reporter=text-lcov > coverage.lcov && codecov --token=0dbca007-3f90-492f-9684-06177863753a",
    "reinstall": "npm run clean && npm install",
    "rebuild": "npm run clean && npm install",
    "dev": "NODE_ENV=development && npm run build && nodemon dist/server.js",
    "heroku-postbuild": "cd client && npm install && npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ijebusoma/My-Diary.git"
  },
  "keywords": [
    "node",
    "express"
  ],
  "author": "Vanessa Osuka",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ijebusoma/My-Diary/issues"
  },
  "engines": {
    "node": "7.10.1"
  },
  "sourceMap": true,
  "homepage": "https://github.com/Ijebusoma/My-Diary#readme",
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/polyfill": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.5",
    "bcrypt": "^3.0.3",
    "body-parser": "^1.18.3",
    "chai-http": "^4.2.1",
    "cors": "^2.8.5",
    "dev": "^0.1.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.3",
    "html-webpack-plugin": "^3.2.0",
    "jsonwebtoken": "^8.3.0",
    "jwt-decode": "^2.2.0",
    "mocha": "^5.2.0",
    "path": "^0.12.7",
    "pg": "^7.4.3",
    "pg-promise": "^5.9.7",
    "prop-types": "^15.6.2",
    "request": "^2.87.0"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^2.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "file-loader": "^2.0.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-loader": "^0.5.5",
    "mini-css-extract-plugin": "^0.4.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.2",
    "style-loader": "^0.23.0",
    "terser-webpack-plugin": "^1.2.3",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.2.1"
  }
}
